@Library('jenkinsLibs') _
pipeline {
    agent any
    stages {
        stage('crear'){
            steps{
                script{
                    mover.crearDirectorio()
                    echo 'termino creacion carpeta'
                }
            }
        }
        stage('clonar'){
            steps{
                script{
                    echo "hola"
                } 
            }
        }
    }
}